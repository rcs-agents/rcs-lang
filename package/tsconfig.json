{
  "extends": "../../tsconfig.esm.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "types": [],
    "tsBuildInfoFile": "./dist/.tsbuildinfo",
    "paths": {
      "@parser/*": ["./src/*"],
      "@parser/generated/*": ["./src/generated/*"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist", "src/**/*.test.ts", "tests/**/*"]
}
