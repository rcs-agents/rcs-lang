{
  "name": "@rcs-lang/ast",
  "version": "0.3.3",
  "description": "Parser-independent AST type definitions for RCL language",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "moon run ast:build",
    "dev": "moon run ast:dev",
    "test": "moon run ast:test",
    "test:watch": "moon run ast:test-watch",
    "test:coverage": "moon run ast:test-coverage",
    "typecheck": "moon run ast:typecheck",
    "clean": "moon run ast:clean",
    "lint": "moon run ast:lint",
    "format": "moon run ast:format"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/minimatch": "^6.0.0",
    "@types/node": "^24.0.13",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
