{
  "name": "@rcs-lang/csm",
  "version": "0.3.5",
  "description": "Lightweight conversation state machine for RCS agents",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "schema",
    "README.md"
  ],
  "scripts": {
    "build": "moon run csm:build",
    "dev": "moon run csm:dev",
    "test": "moon run csm:test",
    "test:watch": "moon run csm:test-watch",
    "test:coverage": "moon run csm:test-coverage",
    "typecheck": "moon run csm:typecheck",
    "clean": "moon run csm:clean",
    "lint": "moon run csm:lint",
    "format": "moon run csm:format",
    "validate-fixture": "bun run validate-fixture.ts"
  },
  "keywords": [
    "state-machine",
    "fsm",
    "conversation",
    "rcs",
    "chat",
    "rcl"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/json-logic-js": "^2.0.7",
    "ajv": "^8.17.1",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@rcs-lang/core": "workspace:*",
    "neverthrow": "^8.0.0",
    "json-logic-js": "^2.0.2"
  },
  "engines": {
    "bun": ">=1.2.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
