{
  "vitest.disableWorkspaceWarning": true,
  "yaml.schemas": {
    ".moon/cache/schemas/tasks.json": [
      ".moon/tasks.yml",
      ".moon/tasks/**/*.yml"
    ],
    ".moon/cache/schemas/project.json": ["**/moon.yml"],
    ".moon/cache/schemas/template.json": ["**/template.yml"],
    ".moon/cache/schemas/workspace.json": ["**/.moon/workspace.yml"],
    ".moon/cache/schemas/toolchain.json": ["**/.moon/toolchain.yml"]
  },
  "files.associations": {
    "*.yml.tera": "jinja-yaml",
    "*.md.tera": "jinja-md",
    "*.json.tera": "jinja-json",
    "*.tera": "jinja"
  },
  "cSpell.words": [
    "archivable",
    "citty",
    "deno",
    "moonrepo",
    "nodenv",
    "packemon",
    "proto",
    "prototools",
    "svallory",
    "tagit",
    "tera",
    "tokilabs",
    "tsbuildinfo",
    "typecheck",
    "vite",
    "vitest"
  ],
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    ".config*": ".git*, prettier.*, babel.*, *config.toml, *config.yaml, *config.yml, *rc, *rc.json, *rc.js, *rc.mjs, *rc.cjs, *rc.ts, *rc.toml, *rc.yaml, *rc.yml, .eslint*, .prettier*, .*",
    "Configuration Files.md": ".git*, prettier.*, babel.*, *config.toml, *config.yaml, *config.yml, *rc, *rc.json, *rc.js, *rc.mjs, *rc.cjs, *rc.ts, *rc.toml, *rc.yaml, *rc.yml, .eslint*, .prettier*, .*",
    "*.yml": "${basename}.*.yml"
  },
  "references.preferredLocation": "peek",
  "typescript.tsdk": "node_modules/typescript/lib",
  "workbench.colorCustomizations": {
    "statusBar.background": "#f3c993",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#eeb265",
    "statusBarItem.remoteBackground": "#f3c993",
    "statusBarItem.remoteForeground": "#15202b",
    "sash.hoverBorder": "#f8e0c1",
    "activityBar.activeBackground": "#f8e0c1",
    "activityBar.background": "#f8e0c1",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#13ac69",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "titleBar.activeBackground": "#f3c993",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#f3c99399",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#f3c993"
}
